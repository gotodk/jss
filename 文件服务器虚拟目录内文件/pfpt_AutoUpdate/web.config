<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
   
        <security>
            <requestFiltering>
                <fileExtensions>
                    <remove fileExtension=".config" />
                 
                </fileExtensions>
            </requestFiltering>
        </security>
        <staticContent>
            <mimeMap fileExtension=".config" mimeType="text/xml" />
        </staticContent>
    </system.webServer>
</configuration>
