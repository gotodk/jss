        </Connections>
        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="DataTable1TableAdapter" Name="DataTable1">
            <MainSource>
              <DbSource ConnectionRef="FMOPConn (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="False" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="False">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT TOP 0
		RKD.CSZX,
		WPBH,
		WPPP,
		GGXH,
			(
				select count(*) from CSZX_HSP_Stock F inner join cszx_HSPRKD R on F.CKDH = R.number where F.WPBH= HSP.WPBH and R.CSZX= RKD.CSZX 
				and F.state=1
				and DateName (Yy,F.RKSJ)+'-'+DateName(Mm,F.RKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())
							
			)  - 
			(
				select count(*) from CSZX_HSP_Stock F inner join cszx_HSPCKD C on F.RCDH=C.number where F.WPBH= HSP.WPBH and C.cszx= RKD.CSZX 
				and F.state=2 
				and	DateName (Yy,F.CKSJ)+'-'+DateName(Mm,F.CKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())
			)
		AS QCKCSL,
		 ISNULL(
					(
						(
							select count(*) from CSZX_HSP_Stock F inner join cszx_HSPRKD R on F.CKDH=R.number where F.WPBH= HSP.WPBH and R.CSZX=RKD.CSZX 
							and F.state=1
							and DateName (Yy,F.RKSJ)+'-'+DateName(Mm,F.RKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())
						) 
						 -
						(
							select count(*) from CSZX_HSP_Stock F inner join cszx_HSPCKD C on F.RCDH = C.number where F.WPBH= HSP.WPBH and C.cszx=RKD.CSZX 
							and F.state=2
							and DateName (Yy,F.CKSJ)+'-'+DateName(Mm,F.CKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())
						)
					)
					*
					(
						select DJ from system_products WHERE system_Products.Number = HSP.WPBH 
					)
				,0)

		AS QCKCJE,
		
		(
				select count(*) from CSZX_HSP_Stock F inner join cszx_HSPRKD R on F.CKDH = R.number where F.WPBH= HSP.WPBH and R.CSZX= RKD.CSZX 
				and F.state=1
				and DateName (Yy,F.RKSJ)+'-'+DateName(Mm,F.RKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())		
		)
		AS BQRKSL,
		 ISNULL(
					(
						(
							select count(*) from CSZX_HSP_Stock F inner join cszx_HSPRKD R on F.CKDH=R.number where F.WPBH= HSP.WPBH and R.CSZX=RKD.CSZX 
							and F.state=1
							and DateName (Yy,F.RKSJ)+'-'+DateName(Mm,F.RKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())
						) 
					)
					*
					(
						select DJ from system_products WHERE system_Products.Number = HSP.WPBH 
					)
				,0)

		AS BQRKJE,
		(
				select count(*) from CSZX_HSP_Stock F inner join cszx_HSPRKD R on F.CKDH = R.number where F.WPBH= HSP.WPBH and R.CSZX= RKD.CSZX 
				and F.state=1
				and DateName (Yy,F.RKSJ)+'-'+DateName(Mm,F.RKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())		
			)
		AS BQXSSL,
		 ISNULL(
					(
						(
							select count(*) from CSZX_HSP_Stock F inner join cszx_HSPRKD R on F.CKDH=R.number where F.WPBH= HSP.WPBH and R.CSZX=RKD.CSZX 
							and F.state=1
							and DateName (Yy,F.RKSJ)+'-'+DateName(Mm,F.RKSJ) = DateName (Yy,getdate())+'-'+DateName(Mm,getdate())
						) 
					)
					*
					(
						select DJ from system_products WHERE system_Products.Number = HSP.WPBH 
					)
				,0)

		AS BQXSJE,
		0 AS KCYLSL,
		0 AS KCYLJE,
		0 AS FXKCSL,
		0 AS FXKCJE,
		0 AS ZTSL,
		0 AS ZTJE,
		0 AS ZZSL,
		0 AS ZZJE,
		0 AS THSL,
		0 AS THJE,
		0 AS BLPSL,
		0 AS BLPJE
FROM CSZX_HSP_Stock HSP 
INNER JOIN cszx_HSPCKD  CKD ON  HSP.CKDH = CKD.Number
INNER JOIN cszx_HSPRKD	RKD ON	HSP. RCDH = RKD.Number
GROUP BY
	RKD.CSZX,WPBH,WPMC,WPPP,GGXH
ORDER BY RKD.CSZX</CommandText>
